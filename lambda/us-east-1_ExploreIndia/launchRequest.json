{
    "type": "APL",
    "version": "1.0",
    "theme": "dark",
    "import": [],
    "resources": [],
    "styles": {},
    "layouts": {},
    "mainTemplate": {
        "parameters": [
            "payload"
        ],
        "items": [
            {
                "type": "Container",
                "width": "100vw",
                "height": "100vh",
                "alignItems": "center",
                "justifyContent": "center",
                "items": [
                    {
                        "type": "Image",
                        "source": "${payload.ExploreIndiaData.backgroundImage.sources[1].url}",
                        "width": "100vw",
                        "height": "100vh",
                        "scale": "best-fill",
                        "position": "absolute"
                    },
                    {
                        "type": "Text",
                        "text": "Welcome, Let's Explore India",
                        "color": "white",
                        "fontSize": "6vw",
                        "fontWeight": "100",
                        "paddingBottom": "10vh"
                    },
                    {
                        "type": "TouchWrapper",
                        "width": "19vw",
                        "height": "9vw",
                        "item": {
                            "type": "Frame",
                            "width": "19vw",
                            "height": "9vw",
                            "borderRadius": "5vw",
                            "borderWidth": "0.75vw",
                            "borderColor": "#FFFFFF",
                            "item": {
                                "type": "Text",
                                "text": "Begin",
                                "width": "19vw",
                                "height": "9vw",
                                "fontWeight": "100",
                                "fontSize": "4vw",
                                "textAlign": "center",
                                "textAlignVertical": "center"
                            }
                        },
                        "onPress": {
                            "type": "SendEvent",
                            "arguments": [
                                "begin"
                            ]
                        }
                    }
                ]
            }
        ]
    }
}